{
    "name": "sakoora0x/laravel-monero-module",
    "description": "Laravel Monero Module",
    "homepage": "https://github.com/sakoora0x/laravel-monero-module",
    "license": "MIT",
    "authors": [
        {
            "name": "sakoora0x",
            "email": "sakoora0x@gmail.com"
        },
        {
            "name": "MollSoft",
            "email": "info@mollsoft.com"
        }
    ],
    "keywords": [
        "sakoo  ",
        "laravel",
        "monero",
        "xmr"
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "brick/math": "*",
        "illuminate/contracts": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "sakoora0x\\LaravelMoneroModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sakoora0x\\LaravelMoneroModule\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sakoora0x\\LaravelMoneroModule\\MoneroServiceProvider"
            ],
            "aliases": {
                "Monero": "sakoora0x\\LaravelMoneroModule\\Facades\\Monero"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
